{
  "name": "card-validation-api",
  "version": "1.0.0",
  "description": "card validator API using Luhn algorithm",
  "main": "dist/src/app.js",
  "scripts": {
    "build": "tsc",
    "test": "NODE_ENV=test jest",
    "lint": "eslint . --ext .ts --max-warnings 0",
    "dev": "docker-compose up",
    "dev:stop": "docker-compose down",
    "synth": "cdk synth",
    "bootstrap": "cdk bootstrap",
    "deploy": "pnpm run build && cdk deploy --require-approval never",
    "destroy": "cdk destroy"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "20.11.14",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "aws-cdk": "^2.138.0",
    "eslint": "^9.39.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.138.0",
    "constructs": "^10.3.0",
    "express": "^4.18.2",
    "source-map-support": "^0.5.21",
    "winston": "^3.18.3"
  },
  "packageManager": "pnpm@10.17.1"
}